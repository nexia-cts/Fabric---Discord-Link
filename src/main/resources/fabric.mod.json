    "sources": "https://github.com/arthurbambou/Fabric---Discord-Link",
    "issues": "https://github.com/arthurbambou/Fabric---Discord-Link/issues",
    "discord": "https://discord.gg/xTvWwzV"
  },

  "license": "MIT",
